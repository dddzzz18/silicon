field f: Int

method foo1(x: Ref, y: Ref)
    requires acc(x.f) && x.f == 2
{
    havoc x != y ==> y.f
        // this havoc does nothing, since we
        // have no heap chunks with access to
        // y.f
    assert x.f == 2
}

method foo2(y: Ref, x0: Ref, x1: Ref)
    requires acc(y.f) && y.f == 42
{
    havoc y == x0 ==> y.f
    havoc y == x1 ==> y.f
    
    assume y != x0 && y != x1
    assert y.f == 42
}
